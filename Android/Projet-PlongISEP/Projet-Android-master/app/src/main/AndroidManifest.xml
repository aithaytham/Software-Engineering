<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.projetand">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".SitesActivity">
        </activity>
        <activity android:name=".ClubActivity">
        </activity>
        <activity android:name=".AddActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MonnCompte"
            android:label="@string/toolbar_title_login_activity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Search_Auto"
            android:label="Recherche auto"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>